{"remainingRequest":"E:\\project\\pro\\pro-vue\\node_modules\\babel-loader\\lib\\index.js!E:\\project\\pro\\pro-vue\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!E:\\project\\pro\\pro-vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!E:\\project\\pro\\pro-vue\\src\\views\\aircraft\\AircraftList.vue?vue&type=script&lang=js","dependencies":[{"path":"E:\\project\\pro\\pro-vue\\src\\views\\aircraft\\AircraftList.vue","mtime":1742091999813},{"path":"E:\\project\\pro\\pro-vue\\babel.config.js","mtime":1742054610233},{"path":"E:\\project\\pro\\pro-vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1742054760353},{"path":"E:\\project\\pro\\pro-vue\\node_modules\\babel-loader\\lib\\index.js","mtime":1742054759432},{"path":"E:\\project\\pro\\pro-vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1742054760353},{"path":"E:\\project\\pro\\pro-vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1742054760025}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["getAircraftList","getAircraft","deleteAircraft","createAircraft","updateAircraft","assignMission","getMissions","recallMission","AircraftForm","name","components","data","loading","error","aircraftList","searchForm","type","currentPage","pageSize","total","dialogVisible","dialogType","assignDialogVisible","assignForm","quantity","missionName","missionType","description","currentRow","missionDialogVisible","missionLoading","missionList","currentAircraft","detailDialogVisible","detailLoading","aircraftDetail","created","fetchData","methods","res","length","message","formatAltitude","val","toLocaleString","formatSpeed","toFixed","getTypeTag","tags","getMissionStatusTag","status","formatMissionStatus","statusMap","handleSearch","resetSearch","handleSizeChange","handleCurrentChange","handleAdd","handleClose","done","$refs","aircraftForm","isFormChanged","$confirm","confirmButtonText","cancelButtonText","customClass","then","catch","handleSubmit","formData","id","originalForm","$notify","title","position","duration","handleAssign","row","handleAssignSubmit","_res$data","$message","success","_error$response","response","handleAssignClose","handleDelete","handleViewMissions","fetchMissions","aircraftId","handleRecall","mission","handleViewDetail","handleEdit","console","log","Error"],"sources":["src/views/aircraft/AircraftList.vue"],"sourcesContent":["<template>\r\n  <div class=\"aircraft-list\">\r\n    <!-- 错误提示 -->\r\n    <div v-if=\"error\" class=\"error-message\">\r\n      <el-alert\r\n        :title=\"error\"\r\n        type=\"error\"\r\n        effect=\"dark\"\r\n        show-icon>\r\n      </el-alert>\r\n    </div>\r\n\r\n    <!-- 搜索区域 -->\r\n    <div class=\"search-panel\">\r\n      <div class=\"panel-header\">\r\n        <div class=\"title\">\r\n          <i class=\"el-icon-plane\"></i>\r\n          <span>飞机管理</span>\r\n        </div>\r\n        <el-button type=\"primary\" @click=\"handleAdd\">\r\n          <i class=\"el-icon-plus\"></i>\r\n          添加飞机\r\n        </el-button>\r\n      </div>\r\n      <div class=\"search-form\">\r\n        <el-form :inline=\"true\" :model=\"searchForm\" class=\"form-content\">\r\n          <el-form-item label=\"飞机名称\">\r\n            <el-input\r\n              v-model=\"searchForm.name\"\r\n              placeholder=\"请输入飞机名称\"\r\n              prefix-icon=\"el-icon-search\"\r\n              clearable>\r\n            </el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"飞机类型\">\r\n            <el-select v-model=\"searchForm.type\" placeholder=\"请选择类型\" clearable>\r\n              <el-option label=\"战斗机\" value=\"战斗机\"></el-option>\r\n              <el-option label=\"运输机\" value=\"运输机\"></el-option>\r\n              <el-option label=\"侦查机\" value=\"侦查机\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" @click=\"handleSearch\">查询</el-button>\r\n            <el-button @click=\"resetSearch\">重置</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 数据表格 -->\r\n    <el-card class=\"table-card\">\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"aircraftList\"\r\n        stripe\r\n        class=\"aircraft-table\">\r\n        <el-table-column\r\n          type=\"index\"\r\n          label=\"序号\"\r\n          width=\"80\"\r\n          align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"name\"\r\n          label=\"飞机名称\"\r\n          min-width=\"120\">\r\n          <template slot-scope=\"scope\">\r\n            <div class=\"aircraft-name\">\r\n              <i class=\"el-icon-plane\"></i>\r\n              {{ scope.row.name }}\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"type\"\r\n          label=\"类型\"\r\n          width=\"100\"\r\n          align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag :type=\"getTypeTag(scope.row.type)\">{{ scope.row.type }}</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"quantity\"\r\n          label=\"数量\"\r\n          width=\"80\"\r\n          align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"flightAltitude\"\r\n          label=\"飞行高度(米)\"\r\n          width=\"110\"\r\n          align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            {{ formatAltitude(scope.row.flightAltitude) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"flightSpeed\"\r\n          label=\"速度(马赫)\"\r\n          width=\"110\"\r\n          align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <span class=\"speed-value\">{{ formatSpeed(scope.row.flightSpeed) }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"stealthLevel\"\r\n          label=\"隐身性能\"\r\n          width=\"110\"\r\n          align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-rate\r\n              :value=\"scope.row.stealthLevel / 2\"\r\n              disabled\r\n              show-score\r\n              text-color=\"#ff9900\">\r\n            </el-rate>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"radarModel\"\r\n          label=\"雷达型号\"\r\n          width=\"120\"\r\n          align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"weaponCapacity\"\r\n          label=\"载弹数量\"\r\n          width=\"100\"\r\n          align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <span v-if=\"scope.row.type === '战斗机'\">{{ scope.row.weaponCapacity }}</span>\r\n            <span v-else>-</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"missionStatus\"\r\n          label=\"任务状态\"\r\n          width=\"100\"\r\n          align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag :type=\"getMissionStatusTag(scope.row.missionStatus)\">\r\n              {{ formatMissionStatus(scope.row.missionStatus) }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"操作\"\r\n          width=\"350\"\r\n          align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"info\"\r\n              @click=\"handleViewDetail(scope.row)\">\r\n              查看详细\r\n            </el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"warning\"\r\n              @click=\"handleEdit(scope.row)\">\r\n              修改\r\n            </el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"handleAssign(scope.row)\">\r\n              分配任务\r\n            </el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"success\"\r\n              @click=\"handleViewMissions(scope.row)\">\r\n              任务列表\r\n            </el-button>\r\n            <el-button\r\n              size=\"mini\"\r\n              type=\"danger\"\r\n              @click=\"handleDelete(scope.row)\">\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页 -->\r\n      <div class=\"pagination-container\">\r\n        <el-pagination\r\n          background\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[10, 20, 30, 50]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </el-card>\r\n\r\n    <!-- 添加飞机对话框 -->\r\n    <el-dialog\r\n      :title=\"dialogType === 'add' ? '添加飞机' : '修改飞机'\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"50%\"\r\n      :before-close=\"handleClose\">\r\n      <aircraft-form\r\n        ref=\"aircraftForm\"\r\n        :aircraft=\"currentAircraft\"\r\n        @submit=\"handleSubmit\"\r\n        @cancel=\"dialogVisible = false\">\r\n      </aircraft-form>\r\n    </el-dialog>\r\n\r\n    <!-- 分配任务对话框 -->\r\n    <el-dialog\r\n      title=\"分配任务\"\r\n      :visible.sync=\"assignDialogVisible\"\r\n      width=\"30%\">\r\n      <el-form :model=\"assignForm\" label-width=\"100px\">\r\n        <el-form-item label=\"飞机名称\">\r\n          <el-input v-model=\"assignForm.name\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"分配数量\">\r\n          <el-input-number \r\n            v-model=\"assignForm.quantity\" \r\n            :min=\"1\" \r\n            :max=\"currentRow ? currentRow.quantity : 1\">\r\n          </el-input-number>\r\n        </el-form-item>\r\n        <el-form-item label=\"任务名称\" required>\r\n          <el-input v-model=\"assignForm.missionName\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"任务类型\" required>\r\n          <el-select v-model=\"assignForm.missionType\" placeholder=\"请选择任务类型\">\r\n            <el-option label=\"战斗任务\" value=\"战斗任务\"></el-option>\r\n            <el-option label=\"侦查任务\" value=\"侦查任务\"></el-option>\r\n            <el-option label=\"运输任务\" value=\"运输任务\"></el-option>\r\n            <el-option label=\"训练任务\" value=\"训练任务\"></el-option>\r\n            <el-option label=\"救援任务\" value=\"救援任务\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"任务描述\">\r\n          <el-input type=\"textarea\" v-model=\"assignForm.description\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"assignDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleAssignSubmit\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n    <!-- 任务列表对话框 -->\r\n    <el-dialog\r\n      title=\"任务列表\"\r\n      :visible.sync=\"missionDialogVisible\"\r\n      width=\"50%\">\r\n      <el-table\r\n        v-loading=\"missionLoading\"\r\n        :data=\"missionList\"\r\n        stripe>\r\n        <el-table-column\r\n          prop=\"aircraftName\"\r\n          label=\"飞机名称\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"aircraftCount\"\r\n          label=\"执行数量\"\r\n          width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"status\"\r\n          label=\"状态\"\r\n          width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag :type=\"getMissionStatusTag(scope.row.status)\">\r\n              {{ formatMissionStatus(scope.row.status) }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"操作\"\r\n          width=\"100\"\r\n          align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              v-if=\"scope.row.status === 'IN_PROGRESS'\"\r\n              size=\"mini\"\r\n              type=\"warning\"\r\n              @click=\"handleRecall(scope.row)\">\r\n              召回\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n\r\n    <!-- 飞机详情对话框 -->\r\n    <el-dialog\r\n      title=\"飞机详细信息\"\r\n      :visible.sync=\"detailDialogVisible\"\r\n      width=\"60%\">\r\n      <div v-loading=\"detailLoading\">\r\n        <el-descriptions :column=\"2\" border>\r\n          <el-descriptions-item label=\"飞机ID\">{{ aircraftDetail.id }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"飞机名称\">{{ aircraftDetail.name }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"飞机类型\">\r\n            <el-tag :type=\"getTypeTag(aircraftDetail.type)\">{{ aircraftDetail.type }}</el-tag>\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"飞机数量\">{{ aircraftDetail.quantity }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"飞行高度(米)\">{{ formatAltitude(aircraftDetail.flightAltitude) }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"飞行速度(马赫)\">{{ formatSpeed(aircraftDetail.flightSpeed) }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"隐身等级\">\r\n            <el-rate\r\n              :value=\"aircraftDetail.stealthLevel / 2\"\r\n              disabled\r\n              show-score\r\n              text-color=\"#ff9900\">\r\n            </el-rate>\r\n          </el-descriptions-item>\r\n          <el-descriptions-item label=\"雷达型号\">{{ aircraftDetail.radarModel }}</el-descriptions-item>\r\n          <el-descriptions-item label=\"任务状态\">\r\n            <el-tag :type=\"getMissionStatusTag(aircraftDetail.missionStatus)\">\r\n              {{ formatMissionStatus(aircraftDetail.missionStatus) }}\r\n            </el-tag>\r\n          </el-descriptions-item>\r\n        </el-descriptions>\r\n        \r\n        <!-- 战斗机特有属性 -->\r\n        <div v-if=\"aircraftDetail.type === '战斗机'\" class=\"specific-info\">\r\n          <h3>战斗机特有属性</h3>\r\n          <el-descriptions :column=\"2\" border>\r\n            <el-descriptions-item label=\"载弹量\">{{ aircraftDetail.weaponCapacity }}</el-descriptions-item>\r\n            <el-descriptions-item label=\"武器类型\">{{ aircraftDetail.weaponTypes }}</el-descriptions-item>\r\n            <el-descriptions-item label=\"作战半径(公里)\">{{ aircraftDetail.combatRange }}</el-descriptions-item>\r\n          </el-descriptions>\r\n        </div>\r\n        \r\n        <!-- 运输机特有属性 -->\r\n        <div v-if=\"aircraftDetail.type === '运输机'\" class=\"specific-info\">\r\n          <h3>运输机特有属性</h3>\r\n          <el-descriptions :column=\"2\" border>\r\n            <el-descriptions-item label=\"载货量(吨)\">{{ aircraftDetail.cargoCapacity }}</el-descriptions-item>\r\n            <el-descriptions-item label=\"货舱容积(立方米)\">{{ aircraftDetail.cargoSpace }}</el-descriptions-item>\r\n            <el-descriptions-item label=\"最大航程(公里)\">{{ aircraftDetail.maxRange }}</el-descriptions-item>\r\n          </el-descriptions>\r\n        </div>\r\n        \r\n        <!-- 侦查机特有属性 -->\r\n        <div v-if=\"aircraftDetail.type === '侦查机'\" class=\"specific-info\">\r\n          <h3>侦查机特有属性</h3>\r\n          <el-descriptions :column=\"2\" border>\r\n            <el-descriptions-item label=\"侦查范围(公里)\">{{ aircraftDetail.reconRange }}</el-descriptions-item>\r\n            <el-descriptions-item label=\"传感器类型\">{{ aircraftDetail.sensorTypes }}</el-descriptions-item>\r\n            <el-descriptions-item label=\"续航时间(小时)\">{{ aircraftDetail.endurance }}</el-descriptions-item>\r\n          </el-descriptions>\r\n        </div>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getAircraftList, getAircraft, deleteAircraft, createAircraft, updateAircraft, assignMission, getMissions, recallMission } from '@/api/aircraft'\r\nimport AircraftForm from '@/components/aircraft/AircraftForm.vue'\r\n\r\nexport default {\r\n  name: 'AircraftList',\r\n  components: {\r\n    AircraftForm\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      error: '',\r\n      aircraftList: [],\r\n      searchForm: {\r\n        name: '',\r\n        type: ''\r\n      },\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      dialogVisible: false,\r\n      dialogType: 'add',\r\n      assignDialogVisible: false,\r\n      assignForm: {\r\n        name: '',\r\n        quantity: 1,\r\n        missionName: '',\r\n        missionType: '',\r\n        description: ''\r\n      },\r\n      currentRow: null,\r\n      missionDialogVisible: false,\r\n      missionLoading: false,\r\n      missionList: [],\r\n      currentAircraft: null,\r\n      detailDialogVisible: false,\r\n      detailLoading: false,\r\n      aircraftDetail: {}\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchData()\r\n  },\r\n  methods: {\r\n    async fetchData() {\r\n      this.loading = true\r\n      try {\r\n        const res = await getAircraftList()\r\n        this.aircraftList = res.data || []\r\n        this.total = this.aircraftList.length\r\n      } catch (error) {\r\n        this.error = error.message\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n    formatAltitude(val) {\r\n      return val ? val.toLocaleString() : '-'\r\n    },\r\n    formatSpeed(val) {\r\n      return val ? val.toFixed(1) : '-'\r\n    },\r\n    getTypeTag(type) {\r\n      const tags = {\r\n        '战斗机': 'danger',\r\n        '运输机': 'success',\r\n        '侦查机': 'warning'\r\n      }\r\n      return tags[type] || 'info'\r\n    },\r\n    getMissionStatusTag(status) {\r\n      const tags = {\r\n        'IN_PROGRESS': 'success',\r\n        'COMPLETED': 'info',\r\n        'ABORTED': 'danger'\r\n      }\r\n      return tags[status] || 'info'\r\n    },\r\n    formatMissionStatus(status) {\r\n      const statusMap = {\r\n        'IN_PROGRESS': '执行中',\r\n        'COMPLETED': '已完成',\r\n        'ABORTED': '已中止'\r\n      }\r\n      return statusMap[status] || status\r\n    },\r\n    handleSearch() {\r\n      this.currentPage = 1\r\n      this.fetchData()\r\n    },\r\n    resetSearch() {\r\n      this.searchForm = {\r\n        name: '',\r\n        type: ''\r\n      }\r\n      this.handleSearch()\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val\r\n      this.fetchData()\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val\r\n      this.fetchData()\r\n    },\r\n    handleAdd() {\r\n      this.dialogType = 'add'\r\n      this.currentAircraft = null\r\n      this.dialogVisible = true\r\n    },\r\n    handleClose(done) {\r\n      if (this.$refs.aircraftForm && this.$refs.aircraftForm.isFormChanged) {\r\n        this.$confirm('表单已修改，确认关闭？', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning',\r\n          customClass: 'dark-message-box'\r\n        }).then(() => {\r\n          done();\r\n        }).catch(() => {\r\n          // 取消关闭\r\n        });\r\n      } else {\r\n        // 如果表单未修改，直接关闭\r\n        done();\r\n      }\r\n    },\r\n    async handleSubmit(formData) {\r\n      try {\r\n        if (this.dialogType === 'add') {\r\n          await createAircraft(formData)\r\n        } else {\r\n          await updateAircraft(this.currentAircraft.id, formData)\r\n        }\r\n        this.$refs.aircraftForm.originalForm = null; // 重置表单状态，避免触发修改确认\r\n        this.dialogVisible = false\r\n        this.fetchData()\r\n        this.$notify({\r\n          title: '成功',\r\n          message: this.dialogType === 'add' ? '添加成功' : '修改成功',\r\n          type: 'success',\r\n          position: 'top-right',\r\n          duration: 2000,\r\n          customClass: 'success-notify'\r\n        })\r\n      } catch (error) {\r\n        if (error !== 'cancel') {\r\n          this.$notify.error({\r\n            title: '错误',\r\n            message: error.message || (this.dialogType === 'add' ? '添加失败' : '修改失败'),\r\n            position: 'top-right',\r\n            duration: 4000\r\n          })\r\n        }\r\n      }\r\n    },\r\n    handleAssign(row) {\r\n      this.currentRow = row\r\n      this.assignForm.name = row.name\r\n      this.assignForm.quantity = 1\r\n      this.assignForm.missionName = `${row.name}的任务`\r\n      this.assignForm.missionType = ''\r\n      this.assignForm.description = ''\r\n      this.assignDialogVisible = true\r\n    },\r\n    async handleAssignSubmit() {\r\n      try {\r\n        if (!this.assignForm.name || !this.assignForm.quantity || !this.assignForm.missionName || !this.assignForm.missionType) {\r\n          this.$message.error('请填写完整信息')\r\n          return\r\n        }\r\n        \r\n        if (this.assignForm.quantity > this.currentRow.quantity) {\r\n          this.$message.error('分配数量不能超过现有数量')\r\n          return\r\n        }\r\n\r\n        const res = await assignMission({\r\n          name: this.assignForm.name,\r\n          quantity: this.assignForm.quantity,\r\n          missionName: this.assignForm.missionName,\r\n          missionType: this.assignForm.missionType,\r\n          description: this.assignForm.description\r\n        })\r\n        this.$message.success(res.data?.message || '任务分配成功')\r\n        this.assignDialogVisible = false\r\n        this.fetchData()\r\n      } catch (error) {\r\n        this.$message.error(error.response?.data?.message || '任务分配失败')\r\n      }\r\n    },\r\n    handleAssignClose() {\r\n      this.assignDialogVisible = false\r\n    },\r\n  